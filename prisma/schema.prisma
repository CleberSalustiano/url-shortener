datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model UrlShortener {
  id Int @default(autoincrement()) @id
  sourceUrl String
  shortnedUrlPath String @unique
  createdAt DateTime @default(now()) 
  updatedAt DateTime @updatedAt
  deletedAt DateTime?
}
